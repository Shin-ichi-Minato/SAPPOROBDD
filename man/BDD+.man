int v4 = BDD_NewVar();
PiDD P1 = PiDD(1).Swap(v1, v2);
PiDD P2 = PiDD(1).Swap(v2, v3);
PiDD P3 = P1 + P2;
PiDD P4 = P1.Swap(v3, v4) + P3;
P3.Print();
P4.Print();

